from abc import ABC, abstractmethod


class Animal(ABC):
    @abstractmethod
    def makeSound(self):
        pass


class Dog(Animal):
    def makeSound(self):
        return "Woof!"


class Cat(Animal):
    def makeSound(self):
        return "Meow!"


def demonstrate_polymorphism(animal: Animal):
    print(animal.makeSound())


dog = Dog()
cat = Cat()


demonstrate_polymorphism(dog)  
demonstrate_polymorphism(cat)   
